<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypes">
      <diagramType
            id="edu.teco.dnd.graphiti.DNDDiagramType"
            name="Data Flow Graph"
            type="dataflowgraph">
      </diagramType>
   </extension>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypeProviders">
      <diagramTypeProvider
            class="edu.teco.dnd.graphiti.DNDDiagramTypeProvider"
            id="edu.teco.dnd.graphiti.DNDDiagramTypeProvider"
            name="Data Flow Editor">
	      <diagramType
	        id="edu.teco.dnd.graphiti.DNDDiagramType">
	      </diagramType>
      </diagramTypeProvider>
   </extension>
   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http:///edu/teco/dnd/graphiti/model.ecore"
            class="edu.teco.dnd.graphiti.model.ModelPackage"
            genModel="model/graphiti.genmodel"/>
   </extension>
   <extension point="org.eclipse.ui.views">
   	     <category
            id="edu.teco.dnd.viewCategory"
            name="DND">
      </category>
      <view id="edu.teco.dnd.eclipse.ViewModule"
    		category="edu.teco.dnd.viewCategory"
            name="Modules"
            class="edu.teco.dnd.eclipse.ViewModule"/>
      <view id="edu.teco.dnd.eclipse.ViewApp"
      		category="edu.teco.dnd.viewCategory"
            name="Applications"
            class="edu.teco.dnd.eclipse.ViewApp"/>
   </extension>
      <extension point="org.eclipse.ui.editors">
      <editor class="edu.teco.dnd.eclipse.deployView.DeployView"
      		extensions="diagram"
      		id="edu.teco.dnd.eclipse.deployView.DeployView"
      		name="Deploy"/>
   </extension>
   <extension point="org.eclipse.ui.preferencePages">
      <page
            class="edu.teco.dnd.eclipse.Preferences"
            id="DND preferences"
            name="DND"/>
      <page
            class="edu.teco.dnd.eclipse.PreferencesNetwork"
            id="DND network preferences"
            name="Network"
            category="DND preferences"/>
   </extension>
</plugin>
